{
	"tags": {
		"allowUnknownTags": ["optional"]
	},
	"plugins": ["node_modules/better-docs/typescript", "plugins/markdown"],
	"source": {
		"include": ["src"],
		"includePattern": "\\.(jsx|js|ts|tsx)$"
	},
	"opts": {
		"destination": "docs",
		"recurse": true,
		"template": "node_modules/better-docs"
	}
}
